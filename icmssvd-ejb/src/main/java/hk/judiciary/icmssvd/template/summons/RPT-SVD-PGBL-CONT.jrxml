			<textField>
				<reportElement x="0" y="20" width="490" height="180" uuid="2115fc93-bc04-4b85-93e8-0ee1d33c939e">
					<printWhenExpression><![CDATA[$F{langType}.equals("E")]]></printWhenExpression>
