package hk.judiciary.icmssvd.model.svdReq.biz.dto;

import java.sql.Timestamp;
import java.util.Date;

import hk.judiciary.icmssvd.model.BaseDTO;
import hk.judiciary.icmssvd.model.common.biz.dto.ComprisingCourtDTO;
import hk.judiciary.icmssvd.model.common.biz.dto.CourtRoomChambersDTO;

/**
 * 
 * @version $Revision: 2551 $ $Date: 2016-12-30 21:14:55 +0800 (Fri, 30 Dec 2016) $
 * @author $Author: vicki.huang $
 */
public class PosRequestDTO extends BaseDTO {
    private static final long serialVersionUID = 1L;
    private Integer posRequestId;
    private Integer requestId;
    private Integer serviceRequestId;
    private Integer successfulTaskId;
    private boolean posDocumentInd;
    private Date posDocumentDueDate;
    private PosDocumentTypeDTO posDocumentType;
    private boolean attendCourtPersonInd;
    private Timestamp posHearingDatetime;
    private Integer requestedBy;
    private ComprisingCourtDTO comprisingCourt;
    private CourtRoomChambersDTO courtRoomChambers;

    public Integer getPosRequestId() {
        return posRequestId;
    }

    public void setPosRequestId(Integer posRequestId) {
        this.posRequestId = posRequestId;
    }

    public Integer getRequestId() {
        return requestId;
    }

    public void setRequestId(Integer requestId) {
        this.requestId = requestId;
    }

    public Integer getServiceRequestId() {
        return serviceRequestId;
    }

    public void setServiceRequestId(Integer serviceRequestId) {
        this.serviceRequestId = serviceRequestId;
    }

    public Integer getSuccessfulTaskId() {
        return successfulTaskId;
    }

    public void setSuccessfulTaskId(Integer successfulTaskId) {
        this.successfulTaskId = successfulTaskId;
    }

    public boolean isPosDocumentInd() {
        return posDocumentInd;
    }

    public void setPosDocumentInd(boolean posDocumentInd) {
        this.posDocumentInd = posDocumentInd;
    }

    public Date getPosDocumentDueDate() {
        return posDocumentDueDate;
    }

    public void setPosDocumentDueDate(Date posDocumentDueDate) {
        this.posDocumentDueDate = posDocumentDueDate;
    }

    public PosDocumentTypeDTO getPosDocumentType() {
        return posDocumentType;
    }

    public void setPosDocumentType(PosDocumentTypeDTO posDocumentType) {
        this.posDocumentType = posDocumentType;
    }

    public boolean isAttendCourtPersonInd() {
        return attendCourtPersonInd;
    }

    public void setAttendCourtPersonInd(boolean attendCourtPersonInd) {
        this.attendCourtPersonInd = attendCourtPersonInd;
    }

    public Timestamp getPosHearingDatetime() {
        return posHearingDatetime;
    }

    public void setPosHearingDatetime(Timestamp posHearingDatetime) {
        this.posHearingDatetime = posHearingDatetime;
    }

    public Integer getRequestedBy() {
        return requestedBy;
    }

    public void setRequestedBy(Integer requestedBy) {
        this.requestedBy = requestedBy;
    }

    public ComprisingCourtDTO getComprisingCourt() {
        return comprisingCourt;
    }

    public void setComprisingCourt(ComprisingCourtDTO comprisingCourt) {
        this.comprisingCourt = comprisingCourt;
    }

    public CourtRoomChambersDTO getCourtRoomChambers() {
        return courtRoomChambers;
    }

    public void setCourtRoomChambers(CourtRoomChambersDTO courtRoomChambers) {
        this.courtRoomChambers = courtRoomChambers;
    }

    @Override
    public String toString() {
        return "PosRequestDTO [posRequestId=" + posRequestId + ", requestId=" + requestId
                + ", serviceRequestId=" + serviceRequestId + ", successfulTaskId="
                + successfulTaskId + ", posDocumentInd=" + posDocumentInd + ", posDocumentDueDate="
                + posDocumentDueDate + ", posDocumentType=" + posDocumentType
                + ", attendCourtPersonInd=" + attendCourtPersonInd + ", posHearingDatetime="
                + posHearingDatetime + ", requestedBy=" + requestedBy + ", comprisingCourt="
                + comprisingCourt + ", courtRoomChambers=" + courtRoomChambers + "]";
    }

}
