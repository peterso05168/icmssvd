package hk.judiciary.icmssvd.model.svdReq.biz.dto;

import java.math.BigDecimal;
import java.util.List;

import hk.judiciary.icmssvd.model.BaseDTO;

/**
 * 
 * @version $Revision: 2551 $ $Date: 2016-12-30 21:14:55 +0800 (Fri, 30 Dec 2016) $
 * @author $Author: vicki.huang $
 */
public class CertifcateBulkPostingDTO extends BaseDTO {
    private static final long serialVersionUID = 1L;
    private String firstClerkName;
    private BigDecimal postageFee;
    private List<ReserviceBatchSearchResultDTO> reserviceBatchSearchResults;

    public String getFirstClerkName() {
        return firstClerkName;
    }

    public void setFirstClerkName(String firstClerkName) {
        this.firstClerkName = firstClerkName;
    }

    public BigDecimal getPostageFee() {
        return postageFee;
    }

    public void setPostageFee(BigDecimal postageFee) {
        this.postageFee = postageFee;
    }

    public List<ReserviceBatchSearchResultDTO> getReserviceBatchSearchResults() {
        return reserviceBatchSearchResults;
    }

    public void setReserviceBatchSearchResults(
            List<ReserviceBatchSearchResultDTO> reserviceBatchSearchResults) {
        this.reserviceBatchSearchResults = reserviceBatchSearchResults;
    }

    @Override
    public String toString() {
        return "CertifcateBulkPostingDTO [firstClerkName=" + firstClerkName + ", postageFee="
                + postageFee + ", reserviceBatchSearchResults=" + reserviceBatchSearchResults + "]";
    }

}
