package hk.judiciary.icmssvd.view.batchPrinting.dataObject
{
	public class BooleanObject extends FieldObject
	{
		public function BooleanObject()
		{
			super();
			value = false;
		}
		
		[Bindable]
		public function get value():Boolean {
			return super.getValue() as Boolean;
		}
		public function set value(value:Boolean):void {
			super.setValue(value as Boolean);
		}
		[Bindable]
		public function get prevValue():Boolean {
			return super.getPrevValue() as Boolean;
		}
		public function set prevValue(value:Boolean):void {
			super.setPrevValue(value as Boolean);
		}
	}
}